{
  "name": "jsreport-sample-template",
  "version": "2.1.0",
  "description": "jsreport extension which creates a sample template at the first run",
  "scripts": {
    "test": "npm run build && mocha test --timeout 20000 && standard",
    "build": "node lib/prepublish.js",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-sample-template",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-sample-template.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "samples/**",
      "/lib/samples/**"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash.omit": "4.5.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-config-standard": "11.0.0",
    "in-publish": "2.0.0",
    "jsreport-assets": "github:jsreport/jsreport-assets",
    "jsreport-chrome-pdf": "1.2.0",
    "jsreport-core": "github:jsreport/jsreport-core",
    "jsreport-data": "2.0.1",
    "jsreport-fs-store": "github:jsreport/jsreport-fs-store",
    "jsreport-handlebars": "2.0.0",
    "jsreport-pdf-utils": "1.0.5",
    "jsreport-scripts": "2.0.7",
    "jsreport-templates": "2.0.0",
    "jsreport-xlsx": "2.0.6",
    "mocha": "5.2.0",
    "puppeteer": "1.11.0",
    "should": "13.2.3",
    "standard": "11.0.1"
  },
  "files": [
    "lib",
    "index.js",
    "samples.json",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}

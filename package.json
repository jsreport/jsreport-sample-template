{
  "name": "jsreport-sample-template",
  "version": "2.2.1",
  "description": "jsreport extension which creates a sample template at the first run",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 4300",
    "test": "npm run build && mocha test --timeout 20000 && standard",
    "build": "node lib/prepublish.js && jsreport-studio-build",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-sample-template",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-sample-template.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "samples/**",
      "/lib/samples/**",
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "lodash.omit": "4.5.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.3.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-assets": "1.1.2",
    "jsreport-chrome-pdf": "1.3.0",
    "jsreport-core": "github:jsreport/jsreport-core",
    "jsreport-data": "2.0.3",
    "jsreport-fs-store": "2.4.0",
    "jsreport-handlebars": "2.0.1",
    "jsreport-pdf-utils": "1.3.1",
    "jsreport-scripts": "2.0.10",
    "jsreport-studio-dev": "1.3.2",
    "jsreport-templates": "2.2.0",
    "jsreport-xlsx": "2.0.10",
    "mocha": "5.2.0",
    "puppeteer": "1.12.2",
    "should": "13.2.3",
    "standard": "11.0.1"
  },
  "files": [
    "lib",
    "index.js",
    "samples.json",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}

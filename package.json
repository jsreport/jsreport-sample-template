{
  "name": "jsreport-sample-template",
  "version": "1.1.2",
  "description": "jsreport extension which creates a sample template at the first run",
  "scripts": {
    "test": "npm run build && mocha test --timeout 10000 && standard",
    "build": "node lib/prepublish.js",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-sample-template",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-sample-template.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "samples/**",
      "/lib/samples/**"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash.omit": "4.5.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "in-publish": "2.0.0",
    "jsreport-assets": "github:jsreport/jsreport-assets#2.0.0-beta",
    "jsreport-chrome-pdf": "github:jsreport/jsreport-chrome-pdf#2.0.0-beta",
    "jsreport-core": "github:jsreport/jsreport-core#2.0.0-beta",
    "jsreport-data": "github:jsreport/jsreport-data#2.0.0-beta",
    "jsreport-fs-store": "github:jsreport/jsreport-fs-store#2.0.0-beta",
    "jsreport-handlebars": "github:jsreport/jsreport-handlebars#2.0.0-beta",
    "jsreport-pdf-utils": "github:jsreport/jsreport-pdf-utils#2.0.0-beta",
    "jsreport-scripts": "github:jsreport/jsreport-scripts#2.0.0-beta",
    "jsreport-templates": "github:jsreport/jsreport-templates#2.0.0-beta",
    "jsreport-xlsx": "github:jsreport/jsreport-xlsx#2.0.0-beta",
    "mocha": "5.1.1",
    "puppeteer": "1.2.0",
    "should": "13.2.1",
    "standard": "11.0.1"
  },
  "files": [
    "lib",
    "index.js",
    "samples.json",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}

{
  "name": "jsreport-sample-template",
  "version": "1.1.3",
  "description": "jsreport extension which creates a sample template at the first run",
  "scripts": {
    "test": "npm run build && mocha test --timeout 10000 && standard",
    "build": "node lib/prepublish.js",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-sample-template",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-sample-template.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "samples/**",
      "/lib/samples/**"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.0",
    "jsreport-import-export": "0.3.1",
    "lodash.omit": "4.5.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "3.19.0",
    "in-publish": "2.0.0",
    "jsreport-assets": "0.6.5",
    "jsreport-core": "1.3.0",
    "jsreport-data": "1.1.2",
    "jsreport-fs-store": "1.2.1",
    "jsreport-handlebars": "1.1.2",
    "jsreport-phantom-pdf": "1.4.3",
    "jsreport-scripts": "1.4.2",
    "jsreport-templates": "1.1.0",
    "jsreport-xlsx": "1.3.1",
    "mocha": "3.2.0",
    "should": "11.2.1",
    "standard": "9.0.1"
  },
  "files": [
    "lib",
    "index.js",
    "samples.json",
    "jsreport.config.js"
  ]
}
